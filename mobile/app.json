{
  "expo": {
    "name": "MacroLens",
    "slug": "macrolens",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.macrolens.app",
      "infoPlist": {}
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.macrolens.app",
      "permissions": []
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-secure-store",
      "expo-font"
    ],
    "scheme": "macrolens"
  }
}